{
  "name": "bondhealth-portal",
  "version": "1.0.0",
  "description": "Healthcare Portal System with Patient Sign In and Admin Dashboard",
  "main": "signin.js",
  "scripts": {
    "start": "node home.js",
    "dev": "nodemon home.js",
    "start:signin": "node signin.js",
    "dev:signin": "nodemon signin.js",
    "start:doctor": "node doctor-registration-server.js",
    "dev:doctor": "nodemon doctor-registration-server.js",
    "start:admin": "node hospital-admin.js",
    "dev:admin": "nodemon hospital-admin.js",
    "dev:all": "concurrently \"npm run dev:signin\" \"npm run dev:doctor\" \"npm run dev:admin\"",
    "start:all": "concurrently \"npm run start:signin\" \"npm run start:doctor\" \"npm run start:admin\""
  },
  "keywords": [
    "healthcare",
    "patient-portal",
    "doctor-registration",
    "admin-dashboard"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.3.1",
    "express": "^4.22.1",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.18.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}
